	#aliases to control the title of an xwindow
	#(from comp.x.windows or something)
	alias dtitle 'echo -n  "]2;${HOST:s=.fsl.noaa.gov==}:$cwd:s=/home/mtn1/moninger/==]1;$cwd:s=/home/mtn1/moninger/=="'
