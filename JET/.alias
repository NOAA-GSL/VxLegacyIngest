#alias python '/lfs1/projects/amb-verif/anaconda/bin/python'

#undo stuff I don't like on bora
unalias cd 

#stuff stolen from Roushey
# list only directories
    alias lsd   '/bin/ls -dF \!* `/bin/ls -F | sed -n "/[@/]/s=[@/]==p"`'
# common shorthand for pushd
    alias pd    pushd
    # common shorthand for popd
    alias pp    popd
    # cause popd to set 'lwd' as a side effect
    alias popd  'popd \!:* ; setlwd'
    # cause pushd to set 'lwd' as a side effect
    alias pushd 'pushd \!:* ; setlwd'
    # this causes the variable 'lwd' to always be the last pushed dir
    alias setlwd        'set dirvar=(`dirs` $cwd); setenv lwd $dirvar[2]'


